function add1 : 
read Op1, Op2 
% 
  Result := ( cons nil Result )  
% 
write Result 
 
function sub1 : 
read Op1, Op2  
% 
  Result := Op1 ;
  Result := (tl Result) 
% 
write Result 
 
function mul : 
read Op1, Op2 
% 
  for Op1 do 
    Result := (add Result Op2) 
  od 
% 
write Result

function test : 
read A
%
  Result := (tl A)
% 
write Result 