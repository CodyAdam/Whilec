function add : 
read Op1, Op2 
% 
  Result := Op1 ; 
  for Op2 do 
    Result := ( cons nil Result )  
  od 
% 
write Result 
 
function sub : 
read Op1, Op2  
% 
  for Op2 do 
    Result := (tl Result) 
  od 
% 
write Result 
 
function mul : 
read Op1, Op2 
% 
  for Op1 do 
    Result := (add Result Op2) 
  od 
% 
write Result