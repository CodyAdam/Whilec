// true constant (for lisibility)
function true :
  read
  %
    Result := (cons nil nil)
  %
write Result

// Logical not
function not :
  read Op1
  %
    if Op1 then Result := (false) else Result := (true) fi
  %
write Result


// Logical and
function and :
  read Op1, Op2
  %
    if (not Op1) then
      Result := (false)
    else
      if (not Op2) then
        Result := (false)
      else
        Result := (true)
      fi
    fi
  %
write Result