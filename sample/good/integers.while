function one :
read
%
    Result := (cons nil nil)
%
write Result

function two :
read
%
    Result := (cons nil (cons nil nil))
%
write Result

function twobis :
read
%
    Result := (cons nil nil nil)
%
write Result

function three :
read
%
    Result := (cons nil (cons nil (cons nil nil)))
%
write Result

function main:
read
%
    N1 := (cons int (one));
    N2 := (cons int (two));
    N2bis := (cons int (twobis));
    N3 := (cons int (three))
%
write N1, N2, N2bis, N3